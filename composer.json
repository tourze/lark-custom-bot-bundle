{
  "name": "tourze/lark-custom-bot-bundle",
  "description": "飞书自定义机器人",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.20 || ^3.0",
    "symfony/framework-bundle": "^6.4",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-track-bundle": "0.0.*",
    "tourze/http-client-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "LarkCustomBotBundle\\": "src"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}